   <string notr="true">QMainWindow {
    background-color: #F2F2F2;
}

QFrame {
    background-color: white;
    border-radius: 6px;
    border: 0px;
}

QLabel {
    color: #1F2937;
}


/* PushButton */

QPushButton {
    background-color: transparent;
    border-radius: 6px;
    border: 1px solid #6B7280;
    color: #1F2937;
    font-weight: normal;
    padding: 6px 8px;
}

QPushButton:hover {
    border: 1px solid #8891A2;
    color: #1F2937;
}

QPushButton:pressed {
    border: 1px solid #1F2937;
    color: #1F2937;
}

QPushButton:disabled {
    border: 1px solid #E9EFFB;
    color: #D3DEF5;
}


/* LineEdit */

QLineEdit {
    background-color: white;
    border-radius: 6px;
    border: 1px solid #CAD1E0;
    color: #1F2937;
    padding: 4px 8px;
}

QLineEdit:focus {
    border: 1px solid #2563EB;
}

QLineEdit:disabled {
    border: 1px solid #F5F8FF;
    color: #D3DEF5;
}


/* SpinBox */

QSpinBox {
    background-color: white;
    border-radius: 6px;
    border: 1px solid #CAD1E0;
    padding: 4px 8px;
    color: #1F2937;
    selection-background-color: #2563EB;
}

QSpinBox:focus {
    border: 1px solid #2563EB;
}

QSpinBox:disabled {
    border: 1px solid #E9EFFB;
    background-color: #F8FAFC;
    color: #9CA3AF;
}

QSpinBox::up-button, QSpinBox::down-button {
    width: 0px;
    height: 0px;
    border: none;
    background: transparent;
}


/* CheckBox */

QCheckBox{
    color: #0F172A;
    font-weight: normal;
    spacing: 8px;
}

QCheckBox::indicator {
    width: 14px;
    height: 14px;
    border-radius: 3px;
    border: 1px solid #64748B;
    background-color: #FFFFFF;
}

QCheckBox::indicator:hover {
    border: 1px solid #2563EB;
}

QCheckBox::indicator:checked {
    border: 1px solid #2563EB;
    background-color: #2563EB;
    image: url(:/icons/checkbox_check);
}

QCheckBox::indicator:focus {
    border: 1px solid #2563EB;
}

QCheckBox::indicator:disabled {
    border: 1px solid #F8FAFC;
    background-color: #FFFFFF;
    image: none;
}

QCheckBox:disabled {
    color: #64748B;
}


/* TableWidget */

QTableView,
QTableWidget {
    background-color: #ffffff;
    border: 1px solid #d7dde9;
    border-radius: 6px;
    gridline-color: #e1e6f2;
    selection-background-color: #2563EB;
    selection-color: #ffffff;
    alternate-background-color: #fafbff;
    color: #1f2937;
}

QTableView::item,
QTableWidget::item {
    padding: 4px 8px;
}

QTableView::item:selected,
QTableWidget::item:selected {
    background-color: #2563EB;
    color: #ffffff;
}

QTableView::item:hover:!selected,
QTableWidget::item:hover:!selected {
    background-color: #eef2fb;
}

QHeaderView::section {
    background-color: #f6f7fb;
    color: #374151;
    padding: 4px 6px;
    border: none;
    border-right: 1px solid #e1e6f2;
    border-bottom: 1px solid #d7dde9;
    font-weight: 600;
}

QHeaderView::section:vertical {
    background-color: #f2f4fa;
    color: #4b5563;
    padding: 2px 4px;
    font-weight: 600;
}

QTableCornerButton::section {
    background-color: #f6f7fb;
    border: none;
    border-right: 1px solid #d7dde9;
    border-bottom: 1px solid #d7dde9;
}

QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 2px 0 2px 0;
}

QScrollBar::handle:vertical {
    background: #d3d9e8;
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background: #c5ccdd;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

QScrollBar:horizontal {
    background: transparent;
    height: 10px;
    margin: 0 2px 0 2px;
}

QScrollBar::handle:horizontal {
    background: #d3d9e8;
    min-width: 20px;
    border-radius: 4px;
}

QScrollBar::handle:horizontal:hover {
    background: #c5ccdd;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0;
}
